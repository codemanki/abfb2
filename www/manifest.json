{
  "manifest_version": 2,
  "name": "Ab fb2 reader",
  "version": "0.30",
  "minimum_chrome_version": "25",
  "permissions": [ "app.window", "*://translate.google.com/*", "storage", {"socket": ["tcp-connect:*:2628"]}],
  "optional_permissions": [ "webRequest" ],
  "icons": {
    "16": "images/b.png",
    "128": "images/A.png"
  },
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  }
}
